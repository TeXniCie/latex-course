                \includegraphics[width=\linewidth,height=0.8\textheight,keepaspectratio]{assets/lijstEnumEnum\langsuffix.pdf}
            \end{column}
        \end{columns}
    \end{frame}

\endDetail

\addtorecentlist{\textbf{itemize}}

\begin{saveblock}{list}
    \begin{highlightblock}[linewidth=0.5\textwidth,gobble=8]
        Dit zijn de ingredi~\"e~nten:
        \begin{itemize}
            \item Wortels
            \begin{enumerate}
                \item Kopen
                \item Raspen
                \item Fijnsnijden
            \end{enumerate}			
            \item Uien
            
            Lipsum dolor sit amet.
            \item Aardappelen
        \end{itemize}
    \end{highlightblock}
\end{saveblock}

\begin{saveblock}{listEN}
    % \begin{codebox}
    %     \begin{minted}{tex}
    %         These are the ingredients:
    %         \begin{itemize}
    %             \item Carrots
    %             \begin{enumerate}
    %                 \item Buy
    %                 \item Peel
    %                 \item Chop
    %             \end{enumerate}
    %             \item Onions
                
    %             Lipsum dolor sit amet.
    %             \item Potatoes
    %         \end{itemize}
    %     \end{minted}
    % \end{codebox}
    \begin{highlightblock}[linewidth=0.5\textwidth,gobble=8]
        These are the ingredients:
        \begin{itemize}
            \item Carrots
            \begin{enumerate}
                \item Buy
                \item Peel
                \item Chop
            \end{enumerate}
            \item Onions
            
            Lipsum dolor sit amet.
            \item Potatoes
        \end{itemize}
    \end{highlightblock}
\end{saveblock}

\begin{frame}[fragile]
    \frametitle{\lang,More with text -- Lists,Lijsten,}
    
    \begin{columns}
        \begin{column}{0.5\textwidth}
            % \lang,\useblock{listEN},\useblock{list},
            \begin{codebox}
                \begin{minted}{tex}
                    These are the ingredients:
                    \begin{itemize}
                        \item Carrots
                        \begin{enumerate}
                            \item Buy
                            \item Peel
                            \item Chop
                        \end{enumerate}
                        \item Onions
                        
                        Lipsum dolor sit amet.
                        \item Potatoes
                    \end{itemize}
                \end{minted}
            \end{codebox}
        \end{column}
        \begin{column}{0.5\textwidth}
            \includegraphics[width=\linewidth,height=0.8\textheight,keepaspectratio]{assets/lijstItemEnum\langsuffix.pdf}
        \end{column}
    \end{columns}
\end{frame}

